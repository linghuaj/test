 <div></div>
<script>
function A(){}
A.prototype.pet = 'dog';
function B {A.call(this);};
B.prototype = Object.create(A.prototype);
B.prototype.constructor = B;

if (A.hasOwnProperty("pet")) {
 alert("A hasOwnProperty pet");
} else {
 alert("A doesn't hasOwnProperty pet");
}
 
if (B["pet"] == "dog") {
 alert("B has pet ", B["pet"]);
}
if (B.hasOwnProperty("pet")) {
 alert("B hasOwnProperty pet");
} else {
 alert ("B doesn't hasOwnProperty pet");
}
 </script>

</body>
